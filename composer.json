{
    "name": "rspaeth/localization",
    "description": "A library for localization and translations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "stichoza/google-translate-php": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "twig/twig": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/MVQN/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rspaeth\\": "./tests/MVQN/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
